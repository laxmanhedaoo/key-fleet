{
  "name": "key-fleet",
  "version": "0.1.0",
  "type": "module",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "test": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@upstash/redis": "^1.25.2",
    "aws-lambda": "^1.0.7",
    "dotenv": "^17.2.3",
    "glob": "^9.3.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
